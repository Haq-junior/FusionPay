{
  "canisters": {
    "fusionPay_backend": {
      "main": "src/fusionPay_backend/main.mo",
      "type": "motoko"
    },
    "fusionPay_frontend": {
      "dependencies": [
        "fusionPay_backend"
      ],
      "frontend": {
        "entrypoint": "src/fusionPay_frontend/src/main.tsx"
      },
      "source": [
        "src/fusionPay_frontend/dist"
      ],
      "type": "assets",
      "build": "cd src/fusionPay_frontend && npm install && npm run build"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}